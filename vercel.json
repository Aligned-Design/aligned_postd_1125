{
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[...all]"
    }
  ],
  "functions": {
    "api/[...all].ts": {
      "maxDuration": 60,
      "includeFiles": [
        "dist/server/vercel-server.mjs",
        "dist/server/vercel-server.js"
      ]
    }
  }
}
