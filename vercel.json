{
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/[...all]"
    }
  ],
  "functions": {
    "api/[...all].ts": {
      "maxDuration": 60,
      "includeFiles": "server/**"
    }
  }
}
